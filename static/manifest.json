{
  "name": "urltool",
  "version": "0.1.0",
  "description": "Url Tool",
  "manifest_version": 2,
  "minimum_chrome_version": "73",
  "permissions": [
    "menus",
    "webNavigation",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "urltool@deadcode.rip",
      "strict_min_version": "52.0"
    }
  }
}
